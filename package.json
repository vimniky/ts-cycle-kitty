{
  "name": "ts-cycle-kitty",
  "version": "0.1.0",
  "description": "A minimalistic Cyclejs + Typescript + Webpack starter kit",
  "keywords": [
    "cycle js",
    "typescript",
    "webpack",
    "mocha",
    "gulp",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vimniky/ts-cycle-kitty"
  },
  "author": "vimniky",
  "license": "MIT",
  "scripts": {
    "start": "run-p client server",
    "client": "cross-env NODE_ENV=development gulp dev & node .internals/webpack/dev.js",
    "server": "nodemon --exec ./node_modules/.bin/ts-node -- ./server/index.ts",
    "build": "run-s clean build:client build:server",
    "mocha": "mocha client/**/*.ts server/**/*.ts --require ts-node/register",
    "build:client": "cross-env NODE_ENV=production gulp build & node .internals/webpack/build.js",
    "build:server": "tsc",
    "test": "cross-env NODE_ENV=development node .internals/test.js",
    "lint": "run-p lint:server lint:client",
    "lint:client": "tslint client/**/*.ts",
    "lint:server": "tslint server/**/*.ts",
    "clean": "run-p clean:client clean:server clean:test",
    "clean:client": "rimraf build/client",
    "clean:server": "rimraf build/server",
    "clean:test": "rimraf test"
  },
  "dependencies": {
    "@cycle/dom": "14.2.0",
    "@cycle/http": "^11.2.0",
    "@cycle/isolate": "^1.4.0",
    "@cycle/xstream-run": "4.1.0",
    "lodash": "^4.17.2",
    "xstream": "9.2.0"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/express": "^4.0.34",
    "@types/mocha": "^2.2.34",
    "@types/node": "^6.0.52",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "cross-spawn": "^5.0.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^1.9.1",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "open-browser-webpack-plugin": "^0.0.3",
    "progress-bar-webpack-plugin": "^1.9.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^1.3.3",
    "ts-node": "^1.7.2",
    "tslint": "next",
    "typescript": "next",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.12"
  }
}
